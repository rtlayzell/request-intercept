{
	"name": "Request Intercept",
	"version": "1.0",
	"description": "Intercept browser requests and make modifications.",
	"manifest_version": 2,
	"permissions": [
		"tabs",
		"storage",
		"webRequest"
	],
	"options_page": "options.html",
	"background": {
		"script": "background.bundle.js"
	},
	"browser_action": {
		"default_popup": "popup.html"
	}
}